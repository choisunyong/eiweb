     Msg.ok(\" \" + res.data.userId + \".\");\r\n        }\r\n        else if (res.result === \"fail\") {\r\n            Msg.error(res.message);\r\n        }\r\n    }\r\n    /**\r\n     *    \r\n     * @param {Object} res {result:ok|fail,message: }\r\n     */\r\n    onResultSearchPw(res) {\r\n        console.log(res);\r\n        if (res.result === \"ok\") {\r\n            Msg.ok(res.message);\r\n        }\r\n        else if (res.result === \"fail\") {\r\n            Msg.error(res.message);\r\n        }\r\n    }\r\n    \r\n    render() {\r\n        const {tabFocus} = this.state;\r\n        return <div className=\"schIdPw mainContent\"><div className={'container'}>\r\n            <div className=\"tab\">\r\n                <div className={\"col-md-6 item\" + (tabFocus === \"schId\" ? \" active\" : \"\")} onClick={() => (tabFocus !== \"schId\" ? this.tabChange('schId') : null)}>ID </div>\r\n                <div className={\"col-md-6 item\" + (tabFocus === \"schPw\" ? \" active\" : \"\")} onClick={() => (tabFocus !== \"schPw\" ? this.tabChange('schPw') : null)}>Password </div>\r\n            </div>\r\n\r\n            {tabFocus === \"schId\"\r\n                ? <div className=\"form\">\r\n                    <div className=\"form\">\r\n                        <input ref=\"userName\" className=\"form-control\" placeholder=\"\" />\r\n                    </div>\r\n                    <div className=\"form\">\r\n                        <input ref=\"schIdEmail\" className=\"form-control\" placeholder=\"\"/>\r\n                    </div>\r\n                    <div className=\"form\">\r\n                        <button className=\"btn btn-success btn-sm\" onClick={() => this.onClickSchId()}> </button>\r\n                    </div>\r\n                </div>\r\n            : null}\r\n            {tabFocus === \"schPw\"\r\n                ? <div className=\"form\">\r\n                <div className=\"form\">\r\n                    <input ref=\"userId\" className=\"form-control\" 
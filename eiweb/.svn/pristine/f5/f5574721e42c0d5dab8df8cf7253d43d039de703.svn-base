.tui-pagination .tui-is-disabled:hover .tui-ico-first {background-position:0 0;}
.tui-pagination .tui-is-disabled .tui-ico-prev,
.tui-pagination .tui-is-disabled:hover .tui-ico-prev {background-position:-3px 0;}
.tui-pagination .tui-is-disabled .tui-ico-next,
.tui-pagination .tui-is-disabled:hover .tui-ico-next {background-position:0 -15px;}
.tui-pagination .tui-is-disabled .tui-ico-last,
.tui-pagination .tui-is-disabled:hover .tui-ico-last {background-position:0 -15px;}

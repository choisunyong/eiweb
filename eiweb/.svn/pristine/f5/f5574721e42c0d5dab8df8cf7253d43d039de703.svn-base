.tui-pagination .tui-ico-next {width:6px;height:10px;background-position:0 -15px;}
.tui-pagination .tui-ico-last {width:9px;height:10px;background-position:0 -15px;}
.tui-pagination .tui-first:hover .tui-ico-first {background-position:-14px 0;}

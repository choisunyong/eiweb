@charset "utf-8";
/*============================================================
Base
=============================================================*/
/* fonts */
@import url('https://fonts.googleapis.com/css?family=Noto+Sans+KR:300,400,500&display=swap&subset=korean');
/* reset */
* {box-sizing:border-box;}
body,div,p,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,table,th,td,pre,form,fieldset,legend,input,textarea,button,select,blockquote {margin:0;padding:0;}
body,input,textarea,select,button,td,th,dt,dd,h1,h2,h3,h4,h5,h6 {font-family:'Noto Sans KR','돋움',Dotum,arial,Helvetica,sans-serif;font-size:13px;color:#666;font-style:normal;font-weight:400;line-height:20px;vertical-align:middle;}
img,fieldset,button,iframe {border:0 none;}
iframe {display:block;}
dl,ul,ol,menu,li {list-style:none;}
address,caption,cite,code,dfn,em,var {font-style:normal;}
table {width:100%;border-collapse:collapse;border-spacing:0;}
caption {visibility:hidden;width:0;height:0;font-size:0;line-height:0;}
legend {position:absolute;top:-1000em;left:0;display:block;overflow:hidden;visibility:hidden;}
:root legend {margin-top:-1px;font-size:0;line-height:0;}
button {border:0 none;outline:none;background-color:transparent;cursor:pointer;}
button:disabled {pointer-events:none;cursor:not-allowed;}
label {margin:0;font-weight:400;cursor:pointer;}
strong {font-weight:500;}
img {vertical-align:top;}
a {color:#777;text-decoration:none;}
a:hover,a:focus,a:active,a:link,a:visited {text-decoration:none;}
hr {display:none;}
.hide {position:absolute;top:-1000em;left:0;display:block;overflow:hidden;visibility:hidden;}

/*============================================================
Common
=============================================================*/

/* font */
.font_gray {color:#999;}
.font_blue {color:#41a0ff;}
/* margin */
.ml5 {margin-left:5px;}
.ml10 {margin-left:10px;}
.ml15 {margin-left:15px;}
.ml20 {margin-left:20px;}
.mt15 {margin-top:15px;}
.mt90 {margin-top:90px;}

/* input */
input::-ms-clear {display:none;}
input[type="search"]::-webkit-search-cancel-button {-webkit-appearance:none;}
input[type="text"],input[type="button"],input[type="password"],input[type="email"],input[type="search"],textarea {outline:none;}
.ipt_text {position:relative;display:block;width:100%;height:32px;padding:4px 12px 6px 12px;border:1px solid #d6d6d6;color:#666;}
.ipt_text:focus {border-color:#2f97ff;color:#333;}
.ipt_text:focus ~ .btn_file {border-color:#2f97ff;color:#333;}
.ipt_text[readonly] {border-color:#f0f0f0;}
.ipt_text[disabled] {border-color:#d6d6d6;background-color:#f5f5f5;}
::-webkit-input-placeholder {color:#c8c8c8 !important;}
:-ms-input-placeholder {color:#c8c8c8 !important;}
.ipt_text2 {display:block;width:100%;height:48px;padding:13px 0;border:0;border-bottom:2px solid #ebebeb;font-size:14px;color:#333;}
.ipt_text2:focus {border-color:#666;}
.ipt_text2::-webkit-input-placeholder {color:#c8c8c8 !important;}
.ipt_text2:-ms-input-placeholder {color:#c8c8c8 !important;}
.ipt_file {position:relative;}
.ipt_file input[type="file"] {position:absolute;top:0;right:0;width:1px;height:1px;padding:0;margin:-1px;overflow:hidden;clip:rect(0,0,0,0);border:0}
.ipt_file .file_text { position: relative; left:32px; border-left:0;}
/* textarea */
textarea {width:100%;padding:12px;border:1px solid #d6d6d6;background-color:#fff;outline:none;resize:none;}
textarea:focus {border-color:#2f97ff;}
textarea[disabled] {border-color:#d6d6d6;background-color:#f5f5f5;}

/* radio, checkbox */
.checkbox {position:relative;}
.checkbox input {position:absolute;top:3px;left:0;width:13px;height:13px;margin:0;opacity:0;cursor:pointer;}
.checkbox input + i {display:inline-block;width:11px;height:11px;margin-top:3px;border:1px solid #bdbdbd;background-color:#fff;vertical-align:top;}
.checkbox input:checked + i {border-color:#419efa;background:#419efa url('../img/check@2x.png') center center no-repeat;background-size:9px 7px;}
.radio_l {position:relative;}
.radio_l input {position:absolute;top:5px;left:0;opacity:0;width:16px;height:16px;margin:0;cursor:pointer;}
/* .radio_l input + i {content:'';display:inline-block;margin-top:5px;margin-right:10px;width:16px;height:16px;background:url('../img/radiobox.png') no-repeat;vertical-align:top;} */
.radio_l input:checked + i {background-position:-26px 0;}
/* toast ui selectbox */
.selectbox {display:inline-block;position:relative;}
.selectbox .tui-select-box-input {height:32px; padding:5px 9px 6px 9px;border:1px solid #d6d6d6;font-size:12px;outline:none;}
.selectbox .tui-select-box-open {border-color:#4097ef;}
.selectbox .tui-select-box-placeholder {color:#666;line-height:20px;vertical-align:top;} 
.selectbox .tui-select-box-icon {width:11px;height:6px;margin-top:7px;background:url('../img/selectbox@2x.png') no-repeat;background-size:11px 17px;;text-indent: -9999999px;}
.selectbox .tui-select-box-open .tui-select-box-placeholder {color:#666;}
.selectbox .tui-select-box-open .tui-select-box-icon {background-position-y:-11px;}
.selectbox .tui-select-box-dropdown {z-index: 16;padding:3px 0 5px 0;border-color:#d6d6d6;box-shadow:0 4px 14px 0 rgba(0,0,0,0.06);background-color: #fff;}
.selectbox .tui-select-box-item {height:inherit;padding:3px 12px; text-align: left; font-size:12px;color:#888;line-height:20px;}
.selectbox .tui-select-box-selected,
.selectbox .tui-select-box-highlight {background-color:transparent;color:#4097ef;font-weight:500;}
/* btn */
.btn {display:inline-block;padding:4px 15px 4px 15px;border:1px solid transparent;font-size:12px;text-align:center;cursor:pointer;}
.table_btn {display:inline-block;width:80px;height: 30px;padding: 3px 15px 5px 15px;border:1px solid transparent;font-size:12px;font-weight:bold;cursor:pointer;text-align: center;}
.table_btn:focus,.table_btn:active:focus,.table_btn.active:focus,.table_btn.focus,.table_btn:active.focus,.table_btn.active.focus,
.btn:focus,.btn:active:focus,.btn.active:focus,.btn.focus,.btn:active.focus,.btn.active.focus {outline:none;outline:none -webkit-focus-ring-color;outline-offset:0;}
.btn_white {border-color:#828282;background-color:#fff;color:#666;}
.btn_white:hover {border-color:#111;color:#111;}
.btn_black {border-color:#4d4d4d;background-color:#333;color:#fff;}
.btn_black:hover {border-color:#111;background-color:#111;}
.btn_blue {border-color:#4097ef;background-color:#4097ef;color:#fff;}
.btn_blue:hover {border-color:#2e84db;background-color:#2e84db;}
.btn_orange {border-color:#f66615;color:#f66615;}
.btn_green {border-color:#07c180;color:#07c180;}
.btn_unselect { width:auto; padding:4px 0 3px 0;  border:0; color:#666;}
.btn_default { width:80px; border-color:#828282; background-color:#fff;color:#666;}
.btn_lineblue {border-color:#828282;background-color:#fff;color:#666;}
.btn_lineblue:hover {border-color:#2e84db;color:#4097ef;}
.btn_lineorange {height:28px; border-color:#828282;background-color:#fff;color:#666;}
.btn_lineorange:hover {border-color:#f66615;color:#f66615;}
.btn_disable { background-color: #f0f0f0; color:#b2b2b2; }
.btn_gray {border-color:#f0f0f0;background-color:#f0f0f0;color:#bdbdbd;}
.btn_gray:hover {border-color:#419efa;background-color:#419efa;color:#fff;}
.btn_linegray {border-color:#828282;background-color:#fff;color:#666;}
.btn[disabled] {border-color:#f0f0f0;background-color:#f0f0f0;color:#bdbdbd;pointer-events:none;cursor:not-allowed;}
.btn_file {position:absolute; left:0; width:32px; height: 32px; border:1px solid #d6d6d6; background:url('../img/icon_search@2x.png') center center no-repeat; background-size: 16px 16px;}
.btn_file.disabled {background-color:#f5f5f5; border-color:#d6d6d6; cursor: default;}
/* Swicth button */
.switch { position: relative; display: inline-block; width: 70px; height: 30px;}
.switch input { opacity: 0; width: 0; height: 0; }
.slider { position: absolute; cursor: pointer; top: 0; left: 0; right: 0; width: 70px; bottom: 0; background-color: #686868; -webkit-transition: .4s; transition: .2s; border-radius: 30px; }
.slider:before {position: absolute;content: "";height: 26px;width: 26px; left:3px; bottom: 2px;background-color: white;-webkit-transition: .2s;transition: .2s;border-radius: 50%;}
input:checked + .slider {right:10px;background-color: #2196F3; width: 70px;}
input:checked + .slider::after {content:"ON";color:#ffffff;position: absolute; right:43px;  line-height: 30px;}
input + .slider::after {content:"OFF";color:#ffffff;position: absolute;line-height: 30px;right: 7px;}
input:focus + .slider { box-shadow: 0 0 1px #2196F3; } 
input:checked + .slider::before { -webkit-transform: translateX(38px); -ms-transform: translateX(38px); transform: translateX(38px); }

/* ipt group */
.ipt_group { position: relative; line-height: 32px; }
.ipt_group:after {content:'';display:block;clear:both;}
.ipt_group .tit,
.ipt_group .txt {float:left;padding:5px 0 7px 0;}
.ipt_group .selectbox,
.ipt_group .ipt_text,
.ipt_group .ipt_text2,
.ipt_group .ipt_file,
.ipt_group .tui-datepicker-input,
.ipt_group .btn {float:left;}
.ipt_group .btn_clear { position: absolute; width:18px; height:18px; top:7px; border:0; background:url('../img/btn_delet_normal@2x.png') no-repeat left center; background-size: 18px 18px; cursor: pointer;}
.ipt_group .btn_clear:hover {background:url('../img/btn_delet_hover@2x.png') no-repeat left center; background-size: 18px 18px;}
.ipt_searchpop { float:left; position: relative;}
.ipt_searchpop .ipt_text { padding-left:44px; }
.ipt_searchpop .btn_file { width:32px!important; left:-15px; }
.ipt_group.disabled .tui-select-box-input {pointer-events: none; background-color:#f5f5f5}

/* radio_group */
.radio_group {position: relative;  }
.radio_group input {position: r; opacity: 0; cursor: pointer;}
.radio_group label {position: relative; left:-16px; padding-left:19px; background:url('../img/icon_radio_normal@2x.png') no-repeat left center;background-size:13px 13px;}
.radio_group input:checked + label {background:url('../img/icon_radio_on@2x.png') no-repeat left center !important;background-size:13px 13px !important;}
.radio_group input:disabled + label {background:url('../img/icon_radio_off@2x.png') no-repeat left center;background-size:13px 13px; cursor: default;}
/*============================================================
layout
=============================================================*/
html, body {width:100%;height:100%;}
/* .page_wrapper {position:relative;width:100%;min-height:100%;min-width:1820px;} */
.page_wrapper {position:relative;width:100%;min-width:1820px;}
.page_header {position:relative;z-index:100; width:100%; height:74px;background-color:#282828;}
.page_header .page_inner {width:100%; min-width: 1600px;}
.page_header .page_inner:after {content:'';display:block;clear:both;}
.page_container {width:100%; min-height:1006px;margin:0 auto; text-align: center;}
.page_footer {position:absolute;bottom:0;width:100%;}
/* page_header */
.page_header .brand {float:left; height:74px;padding:20px 0 20px 50px;}
.page_header .brand::after { display:block; content:""; clear: both;}
.page_header .brand a {display:block;width:129px;height:34px;background:url('../img/logo_black@2x.png') no-repeat;background-size:100% 100%;}
.page_header .gnb * {vertical-align:top;}
.page_header .gnb > ul {position:absolute;top:0;left:50%; transform: translateX(-50%); min-width:805px;}
.page_header .gnb > ul:after {content:'';display:block;clear:both;}
.page_header .gnb > ul > li {position:relative;float:left; height: 74px; padding:25px 70px 0 70px;}
.page_header .gnb > ul > li:first-child { padding-left:0;}
.page_header .gnb > ul > li:last-child { padding-right:0;}
.page_header .gnb > ul > li:nth-child(3) {padding-right:80px;}
.page_header .gnb > ul > li > a {position:relative;display:inline-block;font-size:20px;color:#a9a9a9;font-weight:500;}
.page_header .gnb > ul > li > a:after {content:'';position:absolute;bottom:-1px;left:0;width:0;height:3px;background-color:#282828;opacity:0;-ms-transition:all 0.3s;transition:all 0.3s;transition-timing-function:ease-out;}
.page_header .gnb > ul > li:first-child:hover >a, .page_header .gnb > ul > li:nth-child(2):hover >a {padding-bottom:3px; border-bottom:1px solid #fff;} 
.page_header .gnb > ul > li.active > a { color:#fff;  }
.page_header .gnb > ul > li:hover > a { color:#fff;}
.page_header .gnb > ul > li:hover > a~ul {display: block;}
.page_header .gnb > ul > li:last-child > ul { left:85%;}
.page_header .gnb > ul > li > ul {display:none;position:absolute;top:74px; width:max-content; width:255px; left:50%; transform:translateX(-50%); padding:20px 30px 35px; background-color:#282828;}
.page_header .gnb > ul > li > ul > li {padding:10px 0;}
.page_header .gnb > ul > li > ul > li > a {display:inline-block;font-size:15px;color:#c1c1c1;}
.page_header .gnb > ul > li > ul > li > a:hover {color:#fff; margin-bottom:-1px; border-bottom:1px solid #fff;}


.page_header .login_sts { float:right; position: relative; padding-right:50px; height:74px; padding-top:25px;}
.page_header .login_sts > li {  display: inline-block; height: 100%; font-size:15px; font-weight: 500; color:#999;}
.page_header .login_sts > li > a:hover {color:#fff; border-bottom:1px solid #fff;}
.page_header .login_sts::after {display: block; content: ""; clear: both;}
.page_header .login_sts > li:hover > a~ul.user_menu {display:block;}
.page_header .login_sts .user_menu {display: none; position: absolute; right:23px; top:74px; width:135px; padding:20px 0 35px 30px; background-color:#282828; }
.page_header .login_sts .user_menu li {padding:10px 0;}
.page_header .login_sts .user_menu li > a {display:inline-block;font-size:15px;color:#c1c1c1;}
.page_header .login_sts .user_menu li > a:hover {color:#fff; margin-bottom:-1px; border-bottom:1px solid #fff;}


.page_header2 .brand {padding:36px 0 20px 0;}
.page_header2 .brand a {display:block;width:259px;height:53px;background:url('../img/logo_esp_black@2x.png') no-repeat;background-size:259px 53px;}
.page_header2 .page_name {position:absolute;top:55px;right:0;font-size:20px;color:#666;line-height:30px;}

/* page_container */
.breadcrumb {height:35px; width:100%; text-align:right;color:#999; background-color: #f1f1f1;}
.breadcrumb * {vertical-align:top;}
.breadcrumb .inner {width:1600px; margin:0 auto; padding-top:8px; text-align: right;}
.breadcrumb span {display:inline-block; padding: 0 6px;}
.breadcrumb span:last-child {padding-right:0;color:#666;font-weight:500;}

.layout_contents {width:1600px; margin:0 auto;}

/* page_footer */
.page_footer { position: relative; width:100%; padding-top: 10px; text-align: left; }
.page_footer.dash_footer { background-color: #ebedf1; }
.page_footer .page_inner { padding:22px 50px; border-top:1px solid #d2d2d2; background-color: #fff; }
.page_footer .page_inner >a { display:inline-block;width:129px;height:34px;background:url('../img/logo_black_footer@2x.png') no-repeat;background-size:129px 34px;}
.page_footer .page_inner .footer_aside { display:inline-block; margin-left:100px; height:34px; line-height: 34px; }
.page_footer .page_inner .footer_aside li { float:left; font-size:14px; }
.page_footer .page_inner .footer_aside li:hover { color:#2f97ff; cursor: pointer; }
.page_footer .page_inner .footer_aside::after { content:""; display:block; clear:both;}
.page_footer .page_inner .footer_aside li:first-child::after { content:""; display:inline-block; position: relative; width:1px; height:15px; margin:0 20px -3px 20px; background-color: #d6d6d6; }
.page_footer .page_inner .float { display:inline-block; float:right; position: relative; top:7px; }
.page_footer .page_inner::after { content:""; display:block; clear:both;}
/*============================================================
Content Common
=============================================================*/
.content_title {padding:50px 0 40px;}
.content_title .tit {font-size:28px;color:#111;line-height:40px;font-weight:500; text-align: center;}
.content_title .tit::after {display: block; content: ""; margin:16px auto 0; width:50px; border-top:2px solid #429efa;}
.content_stitle {padding:34px 0 18px 0;}
.content_stitle .tit {font-size:18px;color:#333;font-weight:500;}
/* 검색 조건 */
.search_condition { position: relative; margin-bottom:40px; height:80px; }
.search_condition .search_basis {float:left; width:calc(100% - 82px); height: 100%; padding:24px 0; background-color:#f8f8f8; }
.search_condition .search_basis::after { content:""; display: block; clear:both;}
.search_condition .search_basis .group_center { display: inline-block; margin:0 auto;}
.search_condition .search_basis .txt {font-size:13px;}
.search_condition .search_basis .btn {width:100px;margin-left:20px;height: 32px;line-height: 1.7em;}
.search_condition .search_basis2 { margin-bottom:40px; padding:24px 88px; }
.search_condition .search_basis_wide { float:none; width:100%; }
.search_condition .btn_add { display:inline-block; width:80px; height: 80px; background:url('../img/btn_add@2x.png')no-repeat center; background-size: 22px 22px; background-color: #4097ef;}
.search_condition .btn_add:hover { background-color: #2e84db;}
.search_condition .search_basis .selectbox.custom .tui-select-box-input {padding:5px 9px 6px 0px;}
.search_condition .search_basis .selectbox .tui-select-box-input {padding:5px 9px 6px 9px;}

/* toast ui table */
.table_type1 .tui-grid-container * {box-sizing:border-box;}
.table_type1 .tui-grid-container {font-size:12px;position: relative;}
.table_type1 .tui-grid-border-line-top {background-color:#d6d6d6;}
.table_type1 .tui-grid-border-line-bottom {background: none;}
.table_type1 .tui-grid-border-line-left,
.table_type1 .tui-grid-border-line-right {display:none;}
.table_type1 .tui-grid-table {width:100%;border:0;}
.table_type2 * {overflow: visible! important;}
.table_type1 .tui-grid-head-area {overflow:hidden;border-bottom:1px solid #eaeaea;background-color:#f8f8f8;}
.table_type1 th.tui-grid-cell-head {position:relative;padding:6px 5px 7px 5px;border:0;background-color:#f8f8f8;font-weight:500;word-break:keep-all;white-space:normal;}
.table_type1 th.tui-grid-cell-head:after {content:'';position:absolute;top:19px;right:0;width:1px;height:12px;background-color:#e0e0e0;vertical-align:top;}
.table_type1 .tui-grid-lside-area th.tui-grid-cell-head {padding:6px 0 7px 0;} 
.table_type1 td.tui-grid-cell-head {background-color:transparent;}
.table_type1 .tui-grid-lside-area th.tui-grid-cell-head:first-child:after {display:none;}
.table_type1 .tui-grid-rside-area th.tui-grid-cell-head:last-child:after {display:none;}
.table_type1 .tui-grid-lside-area .tui-grid-column-resize-handle {cursor:default;}
.table_type1 .tui-grid-body-area {background-color: transparent; border-bottom:1px solid #eaeaea; height: auto!important;}
.table_type1 .tui-grid-cell {position: relative;border:0;border-bottom:1px solid #eaeaea;background-color:transparent;line-height:18px;}
.table_type1 .tui-grid-cell-content {position: relative;padding:6px 5px 7px 5px;max-width:200px!important; text-overflow: ellipsis; white-space: nowrap; }
.table_type2 .tui-grid-cell-content {position: relative;padding:16px 5px 16px 18px; max-width:200px!important; text-overflow: ellipsis; white-space: nowrap; }
.table_type3 .tui-grid-cell-content {font-size:15px;padding: 21px 5px 21px 5px;}
.table_type1 .tui-grid-cell-content .link {text-decoration:underline; max-width:200px!important; text-overflow: ellipsis; white-space: nowrap; overflow: hidden;}
.table_type1 .tui-grid-cell-content .link:hover {color:#419efa;}
.table_type1 .tui-grid-lside-area .tui-grid-cell-content {padding:15px 0 16px 0;}
.table_type4 .tui-grid-lside-area .tui-grid-cell-content {padding:15px 10px 16px 0; overflow: visible;}
.table_type1 .tui-grid-rside-area {overflow-x:auto;}
.table_type1 .tui-grid-layer-selection {border:0;background-color:transparent;}
.table_type1 .tui-grid-layer-focus-border,
.table_type1 .tui-grid-layer-focus-deactive .tui-grid-layer-focus-border {display:none;}
.table_type1 .tui-grid-layer-editing {display:none;}
.table_type1 .tui-grid-layer-state {background-color:transparent;border-bottom:1px solid #eaeaea;}
.table_type1 .tui-grid-layer-state p {font-size:12px;color:#666; }

/* toast ui pagenation */
.tui-pagination {margin:20px 0 5px 0;position: relative!important;}
.tui-pagination .tui-page-btn {width:inherit;min-width:18px;padding:7px 2px;border:0;color:#888;}
.tui-pagination .tui-page-btn:hover {background-color:transparent;text-decoration:underline;}
.tui-pagination .tui-is-selected {background-color:transparent;color:#419efa;font-weight:500;}
.tui-pagination .tui-first-child.tui-is-selected {border-left:0;}
.tui-pagination .tui-last-child.tui-is-selected {border-right:0;}
.tui-pagination .tui-prev-is-ellip,
.tui-pagination .tui-next-is-ellip {display:none;}
.tui-pagination .tui-first,
.tui-pagination .tui-prev,
.tui-pagination .tui-next,
.tui-pagination .tui-last {width:28px;height:28px;padding:0;border:1px solid #bdbdbd;}
.tui-pagination .tui-first:hover,
.tui-pagination .tui-prev:hover,
.tui-pagination .tui-next:hover,
.tui-pagination .tui-last:hover {border-color:#419efa;}
.tui-pagination .tui-is-disabled,
.tui-pagination .tui-is-disabled:hover {border-color:#f0f0f0;background-color:#f0f0f0;pointer-events:none;cursor:not-allowed;}
.tui-pagination .tui-first + .tui-prev,
.tui-pagination .tui-next + .tui-last {margin-left:2px;border-left:1px solid #bdbdbd;}
.tui-pagination .tui-first + .tui-prev:hover,
.tui-pagination .tui-next + .tui-last:hover {border-left-color:#419efa;}
.tui-pagination .tui-first + .tui-is-disabled,
.tui-pagination .tui-next + .tui-is-disabled,
.tui-pagination .tui-first + .tui-is-disabled:hover,
.tui-pagination .tui-next + .tui-is-disabled:hover {border-left-color:#f0f0f0;}
.tui-pagination .tui-ico-first,
.tui-pagination .tui-ico-prev,

import React, {Component} from 'react';
import {MenuCode} from 'main';

/**
 * 화면명 : 로고
 * 화면 경로 : 없음.
 * 화면 코드 : 없음.
 * 참고 : 
 */
class Logo extends Component {
    render() {
        const {onMenuChange} = this.props;
        return <div className="brand">
            <h1 className="logo"><a onClick={() => onMenuChange(MenuCode.DASHBOARD)}><span className="hide">ESP Energy Intelligence</span></a></h1>
        </div>;
    }
}

export default Logo;